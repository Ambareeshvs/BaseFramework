            }
            $('.accordion-header').addClass("collapsed");
            $(".btnMenuLogOff").click(function () { $(".menuLogOff")[0].submit(); });
        });
    </script>
    @await RenderSectionAsync("Scripts", required: false)
</body>
</html>
