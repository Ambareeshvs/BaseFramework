    @{
        ViewBag.padding = "";
        ViewBag.left = "";
        ViewBag.dashboard = "";
        ViewBag.aside = "aside-enabled aside-fixed";
        if (User.FindFirstValue(ClaimTypes.NameIdentifier) == null)
        {
            ViewBag.padding = "padding:10rem 0;";
            ViewBag.left = "left:0";
            ViewBag.dashboard = "display:none";
            ViewBag.aside = "";
        }
        else if (User.FindFirstValue(ClaimTypes.NameIdentifier) != null && User.FindFirstValue("Roles") == "F")
        {
            ViewBag.padding = "padding:10rem 0;";
            ViewBag.left = "left:0";
            ViewBag.dashboard = "display:none";
            ViewBag.aside = "";
